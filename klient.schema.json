{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title":"Klient",
    "description":"Ein Klient",
    "type":"object",
    "properties":{
        "id":{
            "description":"The unique identifier for a client",
            "type":"integer"
        },
        "clientName":{
            "description":"Name of the client",
            "type":"string"
        },
        "age":{
            "description":"Age of the client",
            "type":"number"
            "exclusiveMinimum":0
        }
    },
    "required": ["id",  "clientName", "age"]
}
